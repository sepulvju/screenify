{
  "name": "screenify",
  "version": "0.0.1",
  "description": "A new wey to share content to screens.",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon -w server --exec \"babel-node server --presets env\"",
    "clean": "rm -rf dist && mkdir dist",
    "build-server": "babel -s -D -d dist/ server/",
    "build": "npm run lint && npm run clean && npm run build-server",
    "lint": "standard server/**/*.js",
    "test": "npm run lint",
    "start": "npm run build && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sepulvju/screenify/"
  },
  "keywords": [
    "screen",
    "content",
    "video"
  ],
  "author": "sepulvju <jp103@tuta.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "winston": "^3.0.0-rc1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
